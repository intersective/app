{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "v3": {
      "i18n": {
        "sourceLocale": "en-US",
        "locales": {
          "es": {
            "translation": "projects/v3/src/locales/messages.es.xlf",
            "baseHref": "es/"
          },
          "ja": {
            "translation": "projects/v3/src/locales/messages.ja.xlf",
            "baseHref": "ja/"
          }
        }
      },
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/v3",
      "sourceRoot": "projects/v3/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "allowedCommonJsDependencies": [
              "pusher-js",
              "moment",
              "moment-timezone",
              "quill",
              "quill-delta",
              "exif-js",
              "plyr",
              "color-convert",
              "quill-magic-url"
            ],
            "localize": true,
            "i18nMissingTranslation": "warning",
            "i18nDuplicateTranslation": "warning",
            "outputPath": "dist/v3",
            "index": "projects/v3/src/index.html",
            "main": "projects/v3/src/main.ts",
            "polyfills": "projects/v3/src/polyfills.ts",
            "tsConfig": "projects/v3/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/v3/src/assets",
                "output": "./assets"
              },
              {
                "glob": "**/*.svg",
                "input": "node_modules/ionicons/dist/ionicons/svg",
                "output": "./svg"
              }
            ],
            "styles": [
              {
                "input": "projects/v3/src/styles.scss",
                "inject": true
              },
              {
                "input": "projects/v3/src/theme/variables.scss",
                "inject": true
              },
              {
                "input": "projects/v3/src/global.scss",
                "inject": true
              }
            ],
            "scripts": [],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "ms": {
              "localize": ["ms"]
            },
            "custom": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "6mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "20kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/v3/src/environments/environment.ts",
                  "with": "projects/v3/src/environments/environment.custom.ts"
                }
              ],
              "buildOptimizer": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false
            },
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/v3/src/environments/environment.ts",
                  "with": "projects/v3/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "localize": ["en-US"],
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "5.5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "20kb"
                }
              ],
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "ms": {
              "browserTarget": "v3:build:development,ms"
            },
            "custom": {
              "browserTarget": "v3:build:custom"
            },
            "production": {
              "browserTarget": "v3:build:production"
            },
            "development": {
              "browserTarget": "v3:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "v3:build:development"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/v3/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/v3/src/favicon.ico",
              "projects/v3/src/assets"
            ],
            "styles": [
              "projects/v3/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/v3/src/**/*.ts",
              "projects/v3/src/**/*.html"
            ]
          }
        }
      }
    },
    "request": {
      "projectType": "library",
      "root": "projects/request",
      "sourceRoot": "projects/request/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/request/ng-package.json"
          },
          "configurations": {
            "custom": {
              "tsConfig": "projects/request/tsconfig.lib.prod.json"
            },
            "production": {
              "tsConfig": "projects/request/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/request/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/request/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/v3/src/favicon.ico",
              "projects/v3/src/assets"
            ],
            "styles": [
              "projects/v3/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  },
  "schematics": {
    "@ionic/angular-toolkit:component": {
      "styleext": "scss"
    },
    "@ionic/angular-toolkit:page": {
      "styleext": "scss"
    }
  }
}
