    "karma-ng-html2js-preprocessor": "^1.0.0",
    "karma-sonarqube-unit-reporter": "0.0.23",
    "ng-packagr": "^17.3.0",
    "serverless": "^3.38.0",
    "serverless-certificate-creator": "^1.6.0",
