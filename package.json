{
  "name": "practera-app-v2",
  "version": "2.7.0",
  "author": "Practera",
  "repository": "https://github.com/intersective/practera-app-v2",
  "homepage": "https://github.com/intersective/practera-app-v2",
  "description": "AppV2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "poststart": "npm run generate-version",
    "local": "ng serve -c local",
    "postlocal": "npm run generate-version",
    "development": "ng serve -c stage --host 0.0.0.0 --port 8080 --disable-host-check --ssl true",
    "build": "ng build",
    "postbuild": "npm run generate-version",
    "deploy": "ng build --base-href '/practera-app-v2/' && git commit -am 'Build to Github Page' && git subtree push --prefix www origin gh-pages",
    "postdeploy": "npm run generate-version",
    "test": "ng test --no-watch",
    "sonar-test": "ng test --no-watch --code-coverage --browsers ChromeHeadless --karmaConfig=src/karma.conf.sonarci.js",
    "lint": "ng lint",
    "lint-fix": "ng lint --fix",
    "e2e": "ng e2e --port 4300",
    "e2e-dev": "ng e2e -c dev --port 4300",
    "e2e-sandbox": "ng e2e -c sandbox",
    "generate-version": "node ./hooks/post-build.js",
    "pubdocs": "npx compodoc -p tsconfig.json src && cp -R documentation/ www/ && ng deploy --base-href=/practera-app-v2/ --branch=compodoc-pages --no-build",
    "native-icons": "cordova-res android --skip-config --copy && cordova-res ios --skip-config --copy",
    "build-native": "npx cap sync",
    "prebuild-native": "ng build -c alpha"
  },
  "dependencies": {
    "@angular/animations": "~12.1.1",
    "@angular/common": "~12.1.1",
    "@angular/core": "~12.1.1",
    "@angular/forms": "~12.1.1",
    "@angular/platform-browser": "~12.1.1",
    "@angular/platform-browser-dynamic": "~12.1.1",
    "@angular/router": "~12.1.1",
    "@apollo/client": "^3.0.0",
    "@capacitor/android": "3.2.3",
    "@capacitor/app": "^1.0.3",
    "@capacitor/core": "3.2.3",
    "@capacitor/haptics": "^1.0.3",
    "@capacitor/keyboard": "^1.0.3",
    "@capacitor/push-notifications": "^1.0.4",
    "@capacitor/status-bar": "^1.0.3",
    "@ionic-native/core": "^5.0.0",
    "@ionic-native/splash-screen": "^5.0.0",
    "@ionic-native/status-bar": "^5.0.0",
    "@ionic/angular": "^5.5.2",
    "@ionic/storage": "^2.2.0",
    "@trtshen/ngx-embed-video": "^2.0.2",
    "angular2-text-mask": "^9.0.0",
    "apollo-angular": "^2.6.0",
    "capacitor-pusher-beams": "git://github.com/intersective/capacitor-pusher-beams.git#master",
    "cordova-sqlite-storage": "^2.6.0",
    "core-js": "^2.5.4",
    "exif-js": "^2.3.0",
    "filestack-js": "^3.20.0",
    "graphql": "^15.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.24.0",
    "ng-circle-progress": "^1.3.0",
    "ng-intercom": "^8.0.2",
    "plyr": "^3.5.10",
    "pusher-js": "^4.4.0",
    "rxjs": "6.6.0",
    "rxjs-compat": "^6.3.3",
    "ts-md5": "^1.2.4",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.1",
    "@angular/cli": "~12.1.1",
    "@angular/compiler": "~12.1.1",
    "@angular/compiler-cli": "~12.1.1",
    "@angular/language-service": "~12.0.1",
    "@capacitor/cli": "3.2.3",
    "@compodoc/compodoc": "^1.1.11",
    "@ionic/angular-toolkit": "^4.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.149",
    "@types/new-relic-browser": "^0.1072.4",
    "@types/node": "^12.11.1",
    "@types/pusher-js": "^4.2.2",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "angular-cli-ghpages": "^0.6.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cucumber": "^6.0.5",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-htmlfile-reporter": "~0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-junit-reporter": "~1.2.0",
    "karma-ng-html2js-preprocessor": "^1.0.0",
    "karma-sonarqube-unit-reporter": "0.0.23",
    "protractor": "~7.0.0",
    "protractor-cucumber-framework": "^6.2.0",
    "serverless": "^2.50.0",
    "serverless-certificate-creator": "^1.4.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.2.4"
  }
}
