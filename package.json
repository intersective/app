    "exif-js": "^2.3.0",
    "filestack-js": "^3.30.0",
    "graphql": "^16.8.1",
    "ics": "^3.7.2",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.4",
    "ng-circle-progress": "~1.7.1",
    "ng-intercom": "^8.0.2",
    "ngx-quill": "^25.3.2",
