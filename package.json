    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-htmlfile-reporter": "~0.3",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "~1.2.0",
    "karma-ng-html2js-preprocessor": "^1.0.0",
    "karma-sonarqube-unit-reporter": "0.0.23",
